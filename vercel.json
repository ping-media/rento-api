{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ],
    "crons": [
    {
      "path": "/src/api/onboarding/utils/cron",
      "schedule": "0 * * * *"}]
}
